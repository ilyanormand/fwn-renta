{
    "vendor": {
        "name": "Nakosport",
        "currency": "EUR",
        "language": "en"
    },
    "header": {
        "fields": [
            {
                "name": "invoice_number",
                "regex": "Invoice Number:\\s*(\\d+)",
                "group": 1
            },
            {
                "name": "invoice_date",
                "regex": "Invoice Date:\\s*([\\d.]+)",
                "group": 1,
                "type": "date"
            }
        ]
    },
    "table": {
        "strategy": "text_regex",
        "start_marker": "Brand\\s+Product\\s+Flavour\\s+Price",
        "end_marker": "Subtotal",
        "row_pattern": "^(.+?)\\s+(.+?)\\s+(.+?)\\s+([\\d,]+\\.\\d{1,2})\\s*€\\s+(\\d+)\\s+\\d+\\s*%\\s+([\\d,]+\\.\\d{1,2})\\s*€",
        "columns": [
            {
                "name": "brand",
                "regex_group": 1,
                "type": "string"
            },
            {
                "name": "description",
                "regex_group": 2,
                "type": "string"
            },
            {
                "name": "flavour",
                "regex_group": 3,
                "type": "string"
            },
            {
                "name": "unit_price",
                "regex_group": 4,
                "type": "number"
            },
            {
                "name": "quantity",
                "regex_group": 5,
                "type": "number"
            },
            {
                "name": "total",
                "regex_group": 6,
                "type": "number"
            }
        ]
    },
    "footer": {
        "fields": [
            {
                "name": "shipping_fee",
                "regex": "Shipping Costs?.*:\\s*([\\d.,]+)",
                "group": 1,
                "type": "number"
            },
            {
                "name": "subtotal",
                "regex": "Subtotal.*?([\\d.,]+)\\s*€",
                "group": 1,
                "type": "number"
            },
            {
                "name": "total",
                "regex": "Total.*?([\\d.,]+)\\s*€",
                "group": 1,
                "type": "number"
            }
        ]
    }
}