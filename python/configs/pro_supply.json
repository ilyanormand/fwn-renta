{
  "vendor": {
    "name": "Pro Supply",
    "currency": "EUR",
    "language": "en"
  },
  "header": {
    "fields": [
      {
        "name": "invoice_number",
        "regex": "Invoice No\\.?\\s*([A-Z0-9]+)",
        "group": 1
      },
      {
        "name": "invoice_date",
        "regex": "Date:.*?(\\d{1,2}\\.\\d{1,2}\\.\\d{4})",
        "group": 1,
        "type": "date"
      }
    ]
  },
  "table": {
    "strategy": "text_regex",
    "start_marker": "Description\\s+Quantity",
    "end_marker": "Subtotal",
    "row_pattern": "^(.+?)\\s+(\\d+)\\s+([\\d,.]+)\\s+([\\d,.]+)$",
    "columns": [
      {
        "name": "description",
        "regex_group": 1,
        "type": "string"
      },
      {
        "name": "quantity",
        "regex_group": 2,
        "type": "number"
      },
      {
        "name": "unit_price",
        "regex_group": 3,
        "type": "number"
      },
      {
        "name": "total",
        "regex_group": 4,
        "type": "number"
      }
    ]
  },
  "footer": {
    "fields": [
      {
        "name": "subtotal",
        "regex": "Subtotal.*?\\s+([\\d,.]+)",
        "group": 1,
        "type": "number"
      },
      {
        "name": "total",
        "regex": "Invoice\\s*total.*?\\s+([\\d,.]+)",
        "group": 1,
        "type": "number"
      }
    ]
  }
}
