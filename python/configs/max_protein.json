{
  "vendor": {
    "name": "Max Protein",
    "currency": "EUR",
    "language": "es"
  },
  "header": {
    "fields": [
      {
        "name": "invoice_number",
        "regex": "Invoice\\s+([\\w/]+)",
        "group": 1
      },
      {
        "name": "invoice_date",
        "regex": "Invoice\\s+Date:\\s*(\\d{2}/\\d{2}/\\d{4})",
        "group": 1,
        "type": "date"
      }
    ]
  },
  "table": {
    "strategy": "text_regex",
    "start_marker": "Description\\s+Quantity",
    "end_marker": "Page:",
    "row_pattern": "^(?:\\[(.+?)\\]\\s+)?(.+?)\\s+([-]?[\\u200b]?[\\d.,]+)(?:\\s+Units)?\\s+([-]?[\\u200b]?[\\d.,]+)\\s+[\\d.,]+\\s+Intracomunitario.+?([-]?[\\u200b]?[\\d.,]+)\\s+€",
    "columns": [
      {
        "name": "sku",
        "regex_group": 1,
        "type": "string"
      },
      {
        "name": "description",
        "regex_group": 2,
        "type": "string"
      },
      {
        "name": "quantity",
        "regex_group": 3,
        "type": "number"
      },
      {
        "name": "unit_price",
        "regex_group": 4,
        "type": "number"
      },
      {
        "name": "total",
        "regex_group": 5,
        "type": "number"
      }
    ]
  },
  "footer": {
    "fields": [
      {
        "name": "total",
        "regex": "Total\\s+([\\d,.]+)\\s*€",
        "group": 1,
        "type": "number"
      }
    ]
  }
}
