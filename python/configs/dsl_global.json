{
  "vendor": {
    "name": "DSL Global",
    "currency": "EUR",
    "language": "nl"
  },
  "header": {
    "fields": [
      {
        "name": "invoice_number",
        "regex": "Invoicenumber\\s*(\\d+)",
        "group": 1
      },
      {
        "name": "invoice_date",
        "regex": "(\\d{1,2}\\s+\\w+\\s+\\d{4})",
        "group": 1,
        "type": "date"
      }
    ]
  },
  "table": {
    "strategy": "text_regex",
    "start_marker": "Article\\s+Product description",
    "end_marker": "Total\\s+excl",
    "row_pattern": "^(.+?)\\s+(\\d+)\\s+€\\s+([\\d,.]+)\\s+([\\d,]+%)\\s+€\\s+([\\d,.]+)",
    "columns": [
      {
        "name": "description",
        "regex_group": 1,
        "type": "string"
      },
      {
        "name": "quantity",
        "regex_group": 2,
        "type": "number"
      },
      {
        "name": "unit_price",
        "regex_group": 3,
        "type": "number"
      },
      {
        "name": "total",
        "regex_group": 5,
        "type": "number"
      }
    ]
  },
  "footer": {
    "fields": [
      {
        "name": "total",
        "regex": "Total\\s+incl\\.\\s+VAT.*?€\\s*([\\d,.]+)",
        "group": 1,
        "type": "number"
      }
    ]
  }
}