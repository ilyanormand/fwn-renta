{
  "vendor": {
    "name": "Shaker Store",
    "currency": "EUR",
    "language": "en"
  },
  "header": {
    "fields": [
      {
        "name": "invoice_number",
        "regex": "(?:Invoice|Sz√°mla).*?(\\d{4,})",
        "group": 1
      },
      {
        "name": "invoice_date",
        "regex": "(\\d{1,2}[\\.\\/\\-]\\d{1,2}[\\.\\/\\-]\\d{4})",
        "group": 1,
        "type": "date"
      },
      {
        "name": "name",
        "regex": "(FITNESS WORLD NUTRITION|FWN)",
        "group": 1,
        "target": "customer"
      }
    ]
  },
  "table": {
    "strategy": "text_regex",
    "row_pattern": "(?:^\\s*\\d+\\s+([A-Z0-9]+)\\s+(.+)$)|(?:^\\s*(?:[\\d]+\\s+)?([\\d,.]+)pcs/\\s*([\\d,.]+).*?([\\d,.]+)\\s+\\d+\\s+[\\d,.]+\\s+([\\d,.]+)\\s*$)",
    "columns": [
      {
        "name": "sku",
        "regex_group": 1,
        "type": "string"
      },
      {
        "name": "description",
        "regex_group": 2,
        "type": "string"
      },
      {
        "name": "quantity",
        "regex_group": 3,
        "type": "number"
      },
      {
        "name": "unit_price",
        "regex_group": 4,
        "type": "number"
      },
      {
        "name": "total",
        "regex_group": 6,
        "type": "number"
      }
    ]
  },
  "footer": {
    "fields": [
      {
        "name": "subtotal",
        "regex": "Subtotal.*?([\\d.,]+)",
        "group": 1,
        "type": "number"
      },
      {
        "name": "total",
        "regex": "Total.*?([\\d.,]+)",
        "group": 1,
        "type": "number"
      }
    ]
  }
}