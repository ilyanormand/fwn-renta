{
    "vendor": {
        "name": "Yamamoto Nutrition",
        "currency": "EUR",
        "language": "it"
    },
    "header": {
        "fields": [
            {
                "name": "invoice_number",
                "regex": "Doc\\.\\s+No\\.\\s*(\\d+)",
                "group": 1
            },
            {
                "name": "invoice_date",
                "regex": "Doc\\.\\s+Date\\s*(\\d{2}/\\d{2}/\\d{4})",
                "group": 1,
                "type": "date"
            }
        ]
    },
    "table": {
        "strategy": "text_regex",
        "start_marker": "ITEM\\s+DESCRIPTION",
        "end_marker": "The exporter of the goods",
        "row_pattern": "^\\s*(IAF\\d+)\\s+.*?(PZ|KG|LT)\\s+([\\d,]+)\\s+([\\d,]+)\\s+(?:\\d+\\s+)?(?:([\\d,.]+)\\s+)?NI",
        "columns": [
            {
                "name": "sku",
                "regex_group": 1,
                "type": "string"
            },
            {
                "name": "quantity",
                "regex_group": 3,
                "type": "number"
            },
            {
                "name": "unit_price",
                "regex_group": 4,
                "type": "number"
            },
            {
                "name": "total",
                "regex_group": 5,
                "type": "number"
            }
        ]
    },
    "footer": {
        "fields": [
            {
                "name": "subtotal",
                "regex": "TOT\\.\\s+TAXABLE\\s+([\\d,.]+)",
                "group": 1,
                "type": "number"
            },
            {
                "name": "total",
                "regex": "TOTAL\\s+DOCUMENT\\s+([\\d,.]+)",
                "group": 1,
                "type": "number"
            }
        ]
    }
}